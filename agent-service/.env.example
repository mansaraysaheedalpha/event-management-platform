# Redis (use existing Redis service)
REDIS_URL=redis://localhost:6379

# Database (TimescaleDB on port 5437 to avoid conflicts)
DATABASE_URL=postgresql://postgres:password@localhost:5437/agent_db

# Anthropic
ANTHROPIC_API_KEY=your-api-key-here

# LangSmith (optional)
LANGCHAIN_API_KEY=
LANGCHAIN_TRACING_V2=false
LANGCHAIN_PROJECT=engagement-conductor

# Agent
ENGAGEMENT_THRESHOLD=0.6
ANOMALY_WARNING_THRESHOLD=0.6
ANOMALY_CRITICAL_THRESHOLD=0.8
