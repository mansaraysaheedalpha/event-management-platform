{
  "compilerOptions": {
    "moduleResolution": "node16",
    "target": "es6",
    "module": "NodeNext",
    "strict": true,
     "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["node_modules"]
}
